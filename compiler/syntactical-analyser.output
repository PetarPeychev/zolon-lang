Grammar

    0 $accept: script $end

    1 script: main-binding
    2       | binding-list main-binding

    3 binding-list: binding
    4             | binding-list binding

    5 binding: IDENTIFIER '=' function
    6        | IDENTIFIER '=' expression

    7 main-binding: "main" '=' function
    8             | "main" '=' expression

    9 function: parameter-list "->" expression

   10 parameter-list: parameter
   11               | parameter-list parameter

   12 parameter: '$' IDENTIFIER

   13 expression: NUMBER
   14           | IDENTIFIER
   15           | function-application
   16           | expression '+' expression
   17           | expression '-' expression
   18           | expression '*' expression
   19           | expression '/' expression
   20           | '-' expression
   21           | expression '^' expression
   22           | '(' expression ')'

   23 function-application: function '(' expression-list ')'

   24 expression-list: %empty
   25                | expression
   26                | expression-list ',' expression


